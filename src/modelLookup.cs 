// Lookup.cs
[DataContract]
public class Lookup
{
    [DataMember]
    public int Lookup_Id { get; set; }

    [DataMember]
    public string Lookup_Type { get; set; }

    [DataMember]
    public string Lookup_Desc { get; set; }

    [DataMember]
    public string Is_Active { get; set; }

    [DataMember]
    public string Created_By { get; set; }

    [DataMember]
    public DateTime Created_Date { get; set; }
}